{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES6", "ES2015", "dom"],
    "module": "preserve",
    "paths": {
      "$app/*": ["./src/routes/*"],
      "$lib/*": ["./src/lib/*"]
    },
    "strict": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
